// Immage with Card UI Blog Tiles from http://codepen.io/mkurapov/pen/qNQXxz

.wrap
{
  margin: 0 auto;
  width:100%;
  height:100%;
  line-height:130px;
  display:flex;
  align-items:space-around;
  max-width:1200px;
  align: center;
}
.Supertiles
{
margin: 0 auto !important;
  width:380px;
  height:380px;
  margin:5px;
  background-color:#99aeff;
  display:inline-block;
  background-size:cover;
  position:relative;
  cursor:pointer;
  transition: all 0.4s ease-out;
  box-shadow: transparent;
  overflow:hidden;
  color:white;
  font-family:'Roboto';
  
}



.Supertiles img
{
  height:100%;
  width:100%;
  position:absolute;
  top:0;
  left:0;
  z-index:0;
  transition: all 0.4s ease-out;
}
.Supertiles .text
{
/*   z-index:99; */
  position:absolute;
  padding:30px;
  
  height:calc(100% - 60px);
}
.Supertiles h1
{
 
  font-weight:300;
  margin:0;
  text-shadow: 2px 2px 10px rgba(0,0,0,0.3);
}
.Supertiles h2
{
  font-weight:100;
  margin:20px 0 0 0;
  font-style:italic;
   transform: translateX(200px);
}
.Supertiles p
{
  font-weight:300;
  margin:20px 0 0 0;
  line-height: 25px;
/*   opacity:0; */
  transform: translateX(-200px);
  transition-delay: 0.2s;
}
.animate-text
{
  opacity:0;
  transition: all 0.6s ease-in-out;
}
.Supertiles:hover
{
/*   background-color:#99aeff; */
box-shadow: none;
  transform:scale(1);
}
.Supertiles:hover img
{
  opacity: 0.2;
}
.Supertiles:hover .animate-text
{
  transform:translateX(0);
  opacity:1;
}
.dots
{
  position:absolute;
  bottom:20px;
  right:30px;
  margin: 0 auto;
  width:30px;
  height:30px;
  color:currentColor;
  display:flex;
  flex-direction:column;
  align-items:center;
  justify-content:space-around;
  
}

.dots span
{
    width: 5px;
    height:5px;
    background-color: currentColor;
    border-radius: 50%;
    display:block;
  opacity:0;
  transition: transform 0.4s ease-out, opacity 0.5s ease;
  transform: translateY(30px);
 
}

.Supertiles:hover span
{
  opacity:1;
  transform:translateY(0px);
}

.dots span:nth-child(1)
{
   transition-delay: 0.05s;
}
.dots span:nth-child(2)
{
   transition-delay: 0.1s;
}
.dots span:nth-child(3)
{
   transition-delay: 0.15s;
}


