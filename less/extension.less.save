  font-size: 16px;
  padding: 20px;
  color: #fff;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 3;
}
.Sighover .tile2 .details span {
  display: block;
  opacity: 0;
  position: relative;
  top: 100px;
  -webkit-transition-property: top, opacity;
  transition-property: top, opacity;
  -webkit-transition-duration: 0.3s;
          transition-duration: 0.3s;
  -webkit-transition-delay: 0s;
          transition-delay: 0s;
}
.Sighover .tile2 .details .title {
  line-height: 1;
  font-weight: 600;
  font-size: 18px;
}
.Sighover .tile2 .details .info {
  line-height: 1.2;
  margin-top: 5px;
  font-size: 12px;
}
.Sighover .tile2:focus:before,
.Sighover .tile2:focus span, .Sighover .tile2:hover:before,
.Sighover .tile2:hover span {
  opacity: 1;
}
.Sighover .tile2:focus:before, .Sighover .tile2:hover:before {
  top: 50%;
}
.Sighover .tile2:focus span, .Sighover .tile2:hover span {
  top: 0;
}
.Sighover .tile2:focus .title, .Sighover .tile2:hover .title {
  -webkit-transition-delay: 0.15s;
          transition-delay: 0.15s;
}
.Sighover .tile2:focus .info, .Sighover .tile2:hover .info {
  -webkit-transition-delay: 0.25s;
          transition-delay: 0.25s;
